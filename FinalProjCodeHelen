create table Master (PlayerID VARCHAR(10), CoachID VARCHAR(10), HofID VARCHAR(10), FirstName VARCHAR(15), LastName VARCHAR(20), NameNote VARCHAR(35), NameGiven VARCHAR(30), NameNick VARCHAR(40), Height INT, Weight INT, ShootCatch VARCHAR (1), LegendsID VARCHAR(10), IhdbID INT, HrefID VARCHAR(10), FirstNHL INT, LastNHL INT, LastWHA INT, Pos VARCHAR(4), BirthYear INT, BirthMon INT, BirthDay INT, BirthCountry VARCHAR(18), BirthState VARCHAR(15), BirthCity VARCHAR(21), DeathYear INT, DeathMon INT, DeathDay INT, DeathCountry VARCHAR(15), DeathState VARCHAR(15), DeathCity VARCHAR(25));

